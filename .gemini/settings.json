{
	"$schema": "https://raw.githubusercontent.com/google-gemini/gemini-cli/main/schemas/settings.schema.json",
	"mcpServers": {
		"svelte": {
			"command": "npx",
			"args": [
				"-y",
				"@sveltejs/mcp"
			]
		},
		"serena": {
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server"
			]
		}
	}
}
